
{
  "extends": "react-native",
  "ecmaFeatures": {
    "modules": true,
    "arrowFunctions": true,
    "classes": true
  },
  "env": {
    "es6": true
  },
  "parser": "babel-eslint",
  "rules": {
    # "no-unused-vars": ["warn", { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }],
    "import/no-unresolved": "off",
    "react/jsx-wrap-multilines": "off",
    "react/jsx-no-bind": "off",
    "no-undef": "off",
    "prefer-const": "off",
    "no-unused-vars": "off",
    "no-console": "off",
    "no-invalid-this": "off"
  }
}
